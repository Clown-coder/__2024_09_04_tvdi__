{% extends '_base.j2' %}


{% block title%}
產品頁面
{% endblock%}



{% block main%}
<div class='container'>
    <table class='table'>
        <thead>
            <tr>
            <th scope="col">編號</th>
            <th scope="col">城市</th>
            <th scope="col">洲別</th>
            <th scope="col">國家</th>
            <th scope="col">圖片</th>
            </tr>
        </thead>
        <tbody>
            {% for item in citys %}
                <tr>
                    <ul>
                        {% for key in item %}
                            <td>{{item[key]}}</td>
                        {%endfor %}

                    </ul>
                </tr>
            {%endfor%}
            
        </tbody>
    </table>
</div>
{% endblock%}